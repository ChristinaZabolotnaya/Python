group1 = [85, 90, 95] # средние баллы за экзамены в первой группе
group2 = [80, 85, 90] # средние баллы за экзамены во второй группе
group3 = [90, 95, 100] # средние баллы за экзамены в третьей группе

avg1 = sum(group1)/len(group1) # средний балл в первой группе
avg2 = sum(group2)/len(group2) # средний балл во второй группе
avg3 = sum(group3)/len(group3) # средний балл в третьей группе

if avg1 > avg2 and avg1 > avg3:
    print("Лучшая группа - первая")
elif avg2 > avg1 and avg2 > avg3:
    print("Лучшая группа - вторая")
else:
    print("Лучшая группа - третья")
