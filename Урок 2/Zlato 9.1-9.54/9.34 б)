p = int(input("Введите сумму: "))
for i in range(p//10 + 1):
    for j in range((p-10*i)//5 + 1):
        for k in range((p-10*i-5*j)//2 + 1):
            l = p - 10*i - 5*j - 2*k
            print("Купюры по 10 рублей:", i, "Купюры по 5 рублей:", j, "Монеты по 2 рубля:", k, "Монеты по 1 рублю:", l)
