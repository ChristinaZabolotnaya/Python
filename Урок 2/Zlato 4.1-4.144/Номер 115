a = int(input())
b = int(input())
c = int(input())
d = int(input())
e = int(input())
f = int(input())
pos_sum = 0
if a > 0:
  pos_sum += a
if b > 0:
  pos_sum += b
if c > 0:
  pos_sum += c
if d > 0:
  pos_sum += d
if e > 0:
  pos_sum += e
if f > 0:
  pos_sum += f
print("Сумма положительных чисел:", pos_sum)
