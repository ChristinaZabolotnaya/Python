a = float(input())
b = float(input())
c = float(input())
d = float(input())
max_num = a
if b > max_num:
  max_num = b
if c > max_num:
  max_num = c
if d > max_num:
  max_num = d
print("Наибольшее число:", max_num)
min_num = a
if b < min_num:
  min_num = b
if c < min_num:
  min_num = c
if d < min_num:
  min_num = d
print("Наименьшее число:", min_num)
