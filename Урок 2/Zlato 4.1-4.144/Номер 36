num = int(input("Введите натуральное число: "))
last_digit = num % 10
if last_digit % 2 == 0:
  print("Номер заканчивается четной цифрой.")
else:
  print("Номер не заканчивается четной цифрой.")
if last_digit % 2 != 0:
  print("Номер заканчивается нечетной цифрой.")
else:
  print("Номер не заканчивается нечетной цифрой.")
