num = int(input("Введите число num - "))
if num // 10 > num % 10:
  print("Первая цифра больше второй цифры.")
elif num // 10 < num % 10:
  print("Вторая цифра больше первой.")
else:
  print("Две цифры равны.")

if num // 10 == num % 10:
  print("Две цифры одинаковые.")
else:
  print("Две цифры разные.")
