num = int(input("Введите число num - "))
if num // 100 > num % 10:
  print("Первая цифра больше последней цифры.")
else:
  print("Первая цифра не больше последней цифры.")
if num // 100 > num // 10 % 10:
  print("Первая цифра больше второй цифры.")
else:
  print("Первая цифра не больше второй цифры.")
if num // 10 % 10 > num % 10:
  print("Вторая цифра больше последней цифры.")
else:
  print("Вторая цифра не больше последней цифры.")

