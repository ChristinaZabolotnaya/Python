num = int(input("Введите трехзначное число - "))
sum_digits = (num // 100) + (num // 10 % 10) + (num % 10)
product_digits = (num // 100) * (num // 10 % 10) * (num % 10)
if sum_digits >= 10:
  print("Сумма цифр представляет собой двузначное число.")
else:
  print("Сумма цифр не представляет собой двузначное число.")
if product_digits >= 100:
  print("Произведение цифр - трехзначное число.")
else:
  print("Произведение цифр не трехзначное число.")
if num > product_digits:
  print("Число больше, чем произведение его цифр.")
else:
  print("Число не больше, чем произведение его цифр.")
if sum_digits % 5 == 0:
  print("Сумма цифр делится на пять.")
else:
  print("Сумма цифр не делится на пять.")
if sum_digits % num == 0:
  print("Сумма цифр делится на число.")
else:
  print("Сумма цифр не делится на число.")
