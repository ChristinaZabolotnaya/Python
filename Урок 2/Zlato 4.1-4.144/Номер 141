k = int(input())
d = int(input())
if d == 1:
  day = 1
else:
  day = 8 - d
day += (k-1) % 7
if day == 1:
  print("Понедельник")
elif day == 2:
  print("Вторник")
elif day == 3:
  print("Среда")
elif day == 4:
  print("Четверг")
elif day == 5:
  print("Пятница")
elif day == 6:
  print("Суббота")
else:
  print("Воскресенье")
