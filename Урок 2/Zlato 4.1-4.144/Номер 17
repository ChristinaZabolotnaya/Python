birth_year = int(input("Введите число birth_year - "))
birth_month = int(input("Введите число birth_month - "))
current_year = int(input("Введите число current_year - "))
current_month = int(input("Введите число current_month - "))
age = current_year - birth_year
if current_month < birth_month:
  age -= 1
print("Возраст человека - ", age, "лет")
