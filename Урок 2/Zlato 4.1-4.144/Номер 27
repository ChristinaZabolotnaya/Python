num = int(input("Введите число num - "))
sum_cubes = (4**3) * ((num // 10) ** 3) + (8**3) * ((num % 10) ** 3)
square = num ** 2
if square == sum_cubes * 4:
  print("Площадь", num, "в четыре раза больше суммы кубов его цифр.")
else:
  print("Площадь", num, "не в четыре раза больше суммы кубов его цифр.")
