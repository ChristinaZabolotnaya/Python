holidays = {'01.01': 'Новый год', '01.07': 'День Канады', '25.12': 'Рождество'}

day = input("Введите день: ")
month = input("Введите месяц: ")

date = f"{day}.{month}"

if date in holidays:
    print(f"На {date} отмечается праздник {holidays[date]}")
else:
    print("На указанную дату праздники не приходятся")
