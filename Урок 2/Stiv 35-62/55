wavelength = int(input("Введите длину волны (в нм): "))

if wavelength >= 380 and wavelength < 450:
    color = "Фиолетовый"
elif wavelength >= 450 and wavelength < 495:
    color = "Синий"
elif wavelength >= 495 and wavelength < 570:
    color = "Зеленый"
elif wavelength >= 570 and wavelength < 590:
    color = "Желтый"
elif wavelength >= 590 and wavelength < 620:
    color = "Оранжевый"
elif wavelength >= 620 and wavelength <= 750:
    color = "Красный"
else:
    print("Ошибка: значение длины волны за пределами видимой части спектра")
    color = ""

if color != "":
    print(f"Цвет, соответствующий длине волны {wavelength} нм: {color}")
