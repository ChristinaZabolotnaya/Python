n = int(input("Введите натуральное число: "))
has_3 = False
has_2_and_5 = False
while n > 0:
  digit = n % 10
  if digit == 3:
    has_3 = True
  elif digit == 2 or digit == 5:
    has_2_and_5 = True
  n //= 10
if has_3:
  print("В числе есть цифра 3")
else:
  print("В числе нет цифры 3")
if has_2_and_5:
  print("В числе есть цифры 2 и 5")
else:
  print("В числе нет цифр 2 и/или 5")
