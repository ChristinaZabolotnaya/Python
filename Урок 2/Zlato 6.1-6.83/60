n = int(input("Введите натуральное число: "))
max_digit_1 = max(str(n))
str_n = str(n).replace(max_digit_1, "", 1)
max_digit_2 = max(str_n)
min_digit_1 = min(str(n))
str_n = str(n).replace(min_digit_1, "", 1)
min_digit_2 = min(str_n)
print("Максимальные цифры:", max_digit_1, max_digit_2)
print("Минимальные цифры:", min_digit_1, min_digit_2)
