n = int(input("Введите число для проверки: "))
while n % 3 == 0:
  n //= 3
if n == 1:
  print("Число является степенью числа 3")
else:
  print("Число не является степенью числа 3")
n = int(input("Введите число для проверки: "))
while n % 5 == 0:
  n //= 5
if n == 1:
  print("Число является степенью числа 5")
else:
  print("Число не является степенью числа 5")
