g = int(input("Введите первый член геометрической прогрессии: "))
z = int(input("Введите знаменатель геометрической прогрессии: "))
n = int(input("Введите число для проверки: "))
if n == g or (n - g) % z == 0 and (n - g) / z >= 0:
  print("Число является членом геометрической прогрессии")
else:
  print("Число не является членом геометрической прогрессии")
