numbers = [8, 8, 8, 8, int(input("Введите число A: "))]
if numbers[-1] == 0:
  print("В новой последовательности максимальных элементов будет 4")
elif numbers[-1] == 8:
  print("В новой последовательности максимальных элементов будет 5")
else:
  print("В новой последовательности максимальных элементов будет 4")
