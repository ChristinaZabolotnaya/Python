sequence = []
for i in range(15):
  sequence.append(float(input(f"Введите число {i+1}: ")))
filtered_sequence = [x for x in sequence if x > 10]
if len(filtered_sequence) > 0:
  mean = sum(filtered_sequence) / len(filtered_sequence)
  print(f"Среднее арифметическое чисел, больших 10: {mean}")
else:
  print("Среди заданных чисел нет чисел, больших 10.")
